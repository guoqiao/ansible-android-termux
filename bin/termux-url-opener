#!/data/data/com.termux/files/usr/bin/bash

export FILES=/data/data/com.termux/files
export HOME=$FILES/home
export BIN=$HOME/bin

export TERMUX_STORAGE=$HOME/storage
export TERMUX_MUSIC=$TERMUX_STORAGE/music
export TERMUX_MOVIES=$TERMUX_STORAGE/movies
export TERMUX_DOWNLOADS=$TERMUX_STORAGE/downloads

python $HOME/bin/termux-url-opener.py --wait-on-finish "$@"
